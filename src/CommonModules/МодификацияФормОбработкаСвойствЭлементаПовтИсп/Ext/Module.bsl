Функция ЗначенияБулево()
	РезультатФункции = Новый Соответствие;
	
	// русский
	РезультатФункции.Вставить(ВРег("Истина"), Истина);
	РезультатФункции.Вставить(ВРег("Ложь"), Ложь);
	РезультатФункции.Вставить(ВРег("Да"), Истина);
	РезультатФункции.Вставить(ВРег("Нет"), Ложь);

	// английский
	РезультатФункции.Вставить(ВРег("True"), Истина);
	РезультатФункции.Вставить(ВРег("False"), Ложь);
	РезультатФункции.Вставить(ВРег("Yes"), Истина);
	РезультатФункции.Вставить(ВРег("No"), Ложь);

	// Числа
	РезультатФункции.Вставить("1", Истина);
	РезультатФункции.Вставить("0", Ложь);
	
	Возврат РезультатФункции;
КонецФункции // ЗначенияБулево()

